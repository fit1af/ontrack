<!DOCTYPE html>
<html>
<head>
  <title>OntrackFit1af</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash %>
    <% flash.each do |type, msg| %>
      <div id="<%= type %>">
        <%= msg %>
      </div>
    <% end %>
  <% end %>

<%= yield %>

</body>
</html>
