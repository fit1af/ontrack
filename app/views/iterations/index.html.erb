<h1>Listing iterations for Project: <%= @iterations.first.project.name %></h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iterations.each do |iteration| %>
  <tr>
    <td><%= iteration.name %></td>
    <td><%= iteration.description %></td>
    <td><%= iteration.start_date %></td>
    <td><%= iteration.end_date %></td>
    <td><%= iteration.project_id %></td>
    <td><%= link_to 'Show', iteration %></td>
    <td><%#= link_to 'Edit', edit_iteration_path(iteration) %></td>
    <td><%#= link_to 'Destroy', iteration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

